package com.bear.dto;

import com.bear.entity.Site;

public class AddSiteReq {

	private Site site;
	
	private String siteUser;
	private String siteUserPwd;
	public Site getSite() {
		return site;
	}
	public void setSite(Site site) {
		this.site = site;
	}
	public String getSiteUser() {
		return siteUser;
	}
	public void setSiteUser(String siteUser) {
		this.siteUser = siteUser;
	}
	public String getSiteUserPwd() {
		return siteUserPwd;
	}
	public void setSiteUserPwd(String siteUserPwd) {
		this.siteUserPwd = siteUserPwd;
	}
	
}
