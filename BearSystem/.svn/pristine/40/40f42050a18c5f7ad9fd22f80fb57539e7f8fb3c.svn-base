<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bear.dao.SiteUserDao">
   <select id="querySiteUser"  resultType="com.bear.entity.SiteUser" >
   		select * from t_site_user
   		where site_user_name=#{0} and site_user_pwd=#{1}
   </select>
</mapper>